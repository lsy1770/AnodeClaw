{
  "name": "anode-clawdbot",
  "version": "1.0.9",
  "description": "Android AI Agent System powered by Claude",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "anode-clawdbot": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && xcopy assets\\prompts dist\\assets\\prompts\\ /E /I /Y",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "start:ui": "node dist/start-ui.js"
  },
  "keywords": [
    "android",
    "ai",
    "agent",
    "claude",
    "automation",
    "anode",
    "chatbot",
    "assistant"
  ],
  "author": "Anode ClawdBot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anode-clawdbot/anode-clawdbot"
  },
  "bugs": {
    "url": "https://github.com/anode-clawdbot/anode-clawdbot/issues"
  },
  "homepage": "https://github.com/anode-clawdbot/anode-clawdbot#readme",
  "files": [
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.1",
    "json5": "^2.2.3",
    "openai": "^4.0.0",
    "wechaty": "^1.20.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@anode177/mcp-client": "^1.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "optionalDependencies": {
    "@larksuiteoapi/node-sdk": "^1.35.0",
    "@types/node-telegram-bot-api": "^0.64.0",
    "dingtalk-stream-sdk-nodejs": "^1.0.0",
    "discord.js": "^14.14.0",
    "file-box": "^1.0.0",
    "node-telegram-bot-api": "^0.66.0",
    "qq-guild-bot": "^2.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "android",
    "linux",
    "darwin",
    "win32"
  ]
}
